{
  "name": "@vinayakkulkarni/v-intl",
  "version": "1.0.4",
  "description": "Native i18n components for your shiny Vue 3 apps",
  "files": [
    "dist"
  ],
  "private": false,
  "main": "dist/v-intl.js",
  "module": "dist/v-intl.esm.js",
  "unpkg": "dist/v-intl.js",
  "jsdelivr": "dist/v-intl.js",
  "cdn": "dist/v-intl.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "lint": "npm run lint:es && npm run lint:prettier",
    "lintfix": "npm run lint:es:fix && npm run lint:prettier:fix",
    "lint:es": "eslint 'src/**/*.{js,vue}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,vue}'",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,vue}\"",
    "release": "shipjs prepare",
    "test": "echo 'No tests found'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "shipjs": "^0.24.0",
    "vue": "^3.0.4",
    "vuepress": "^1.7.1"
  },
  "license": "MIT",
  "author": {
    "name": "vinayakkulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/v-intl",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-intl/issues"
  },
  "homepage": "https://v-intl.netlify.app/"
}
