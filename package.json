{
  "name": "@vinayakkulkarni/v-intl",
  "version": "0.4.3",
  "description": "Native i18n components for your shiny Vue 3 apps",
  "files": [
    "dist"
  ],
  "private": false,
  "main": "dist/v-intl.js",
  "module": "dist/v-intl.esm.js",
  "unpkg": "dist/v-intl.js",
  "jsdelivr": "dist/v-intl.js",
  "cdn": "dist/v-intl.js",
  "sideEffects": false,
  "scripts": {
    "test": "ava",
    "test:snapshot": "ava --update-snapshots",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "lint": "npm run lint:es && npm run lint:prettier",
    "lintfix": "npm run lint:es:fix && npm run lint:prettier:fix",
    "lint:es": "eslint 'src/**/*.{js,vue}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,vue}'",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,vue}\"",
    "release": "shipjs prepare",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^1.1.0",
    "ava": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.1.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "rollup": "^2.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "shipjs": "^0.21.0",
    "vue": "^3.0.0",
    "vuepress": "^1.5.4"
  },
  "license" : "MIT",
  "author": {
    "name": "vinayakkulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/v-intl",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-intl/issues"
  },
  "homepage": "https://v-intl.netlify.app/"
}
